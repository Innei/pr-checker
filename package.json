{
  "name": "pr-checker",
  "description": "",
  "private": false,
  "type": "module",
  "version": "1.0.0",
  "packageManager": "^pnpm@7.0.0",
  "keywords": [],
  "license": "MIT",
  "author": "",
  "homepage": "",
  "repository": "",
  "bugs": "",
  "main": "./index.js",
  "module": "./index.js",
  "types": "./index.d.ts",
  "exports": {
    ".": {
      "types": "./index.d.ts",
      "require": "./index.cjs",
      "import": "./index.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "./*",
        "./index.d.ts"
      ]
    }
  },
  "bin": {
    "pr-checker": "bin/index.js"
  },
  "scripts": {
    "init": "pnpm i",
    "lint:fix": "eslint . --fix",
    "dev": "cross-env RUNTIME_ENV=development esno watch play/index.ts",
    "build": "pnpm run clean && cross-env RUNTIME_ENV=production pnpm run --filter @pr-checker/build build",
    "prepublishOnly": "pnpm run build",
    "release": "bumpp package.json --commit --push --tag && pnpm -r publish --access public",
    "publish": "pnpm run release && pnpm publish --no-git-checks --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "clean": "rimraf dist",
    "test": "vitest",
    "test:update": "vitest -u",
    "test:coverage": "vitest --coverage"
  },
  "peerDependencies": {
    "chalk": "^5.2.0",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2"
  },
  "dependencies": {
    "chalk": "^5.2.0",
    "fs-extra": "^10.0.1",
    "ora": "^6.1.2"
  },
  "devDependencies": {
    "@baiwusanyu/eslint-config": "^1.0.10",
    "@changesets/cli": "^2.26.0",
    "@commitlint/cli": "^17.4.2",
    "@commitlint/config-conventional": "^17.4.2",
    "@pr-checker/build": "workspace:*",
    "@pr-checker/entry": "workspace:*",
    "@pr-checker/play": "workspace:*",
    "@pr-checker/utils": "workspace:*",
    "@rollup/plugin-node-resolve": "^15.0.1",
    "@types/fs-extra": "^11.0.1",
    "@types/gulp": "^4.0.10",
    "@types/node": "^18.0.0",
    "@types/chalk": "^2.2.0",
    "@vitest/coverage-c8": "^0.23.4",
    "@vitest/ui": "^0.23.4",
    "bumpp": "^8.2.1",
    "conventional-changelog-cli": "^2.2.2",
    "cross-env": "^7.0.3",
    "eslint": "8.32.0",
    "esno": "^0.16.3",
    "gulp": "^4.0.2",
    "jsdom": "^20.0.0",
    "lint-staged": "^13.1.1",
    "rimraf": "^4.1.0",
    "rollup": "^3.10.0",
    "rollup-plugin-dts": "^5.1.1",
    "rollup-plugin-typescript2": "^0.34.1",
    "simple-git-hooks": "^2.8.1",
    "tsup": "^6.5.0",
    "typescript": "4.9.4",
    "vitest": "^0.23.4"
  },
  "lint-staged": {
    "*.{vue,js,ts,jsx,tsx,md,json}": [
      "eslint ./ --fix",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "@baiwusanyu"
    ]
  }
}
